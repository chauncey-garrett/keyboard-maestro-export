              <key>Text</key>
              <string>&#13;on appIsRunning(appName)&#13;	tell application "System Events" to (name of processes) contains appName&#13;end appIsRunning&#13;&#13;on runApp(appName)&#13;	if not appIsRunning(appName) then&#13;		log "Running " &amp; appName&#13;		try&#13;			launch application appName&#13;		on error&#13;			log "Error Launching " &amp; appName&#13;		end try&#13;	end if&#13;end runApp&#13;&#13;on basename(the_file)&#13;	set the_path to quoted form of (POSIX path of the_file) as string&#13;	set cmd to ("php -r \"echo pathinfo(" &amp; the_path &amp; ", PATHINFO_FILENAME);\"")&#13;	set output to do shell script cmd&#13;	return output&#13;end basename&#13;&#13;on file_exists(unix_path)&#13;	tell application "Finder"&#13;		exists my POSIX file (unix_path)&#13;	end tell&#13;end file_exists&#13;&#13;set appList to {"Dropbox", "Flux", "Caffeine", "Skitch", ¬&#13;	"TextExpander", "iTeleport Connect", "Tags", ¬&#13;	"/Applications/Keyboard Maestro.app/Contents/Resources/Keyboard Maestro Engine.app", ¬&#13;	"/Library/PreferencePanes/Growl.prefPane/Contents/Resources/GrowlHelperApp.app", ¬&#13;	"/Applications/HoudahSpot.app/Contents/Resources/HoudahSpotHelper.app", ¬&#13;	"/Applications/iTunes.app/Contents/Resources/iTunesHelper.app"}&#13;&#13;repeat with appPath in appList&#13;	if file_exists(appPath) then&#13;		set appName to basename(appPath)&#13;		runApp(appName)&#13;	else&#13;		set appName to appPath&#13;		set appPath to "/Applications/" &amp; appName &amp; ".app"&#13;		if file_exists(appPath) then&#13;			runApp(appName)&#13;		else&#13;			log "Could not find App: " &amp; appName&#13;		end if&#13;	end if&#13;end repeat&#13;</string>
